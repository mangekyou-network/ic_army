service : {
  analyzeMarket: (text, float64) -> (text);
  getAgentInfo: () ->
   (record {
      agentType: variant {
                   MarketSentiment;
                   RiskManager;
                   TechnicalAnalyst;
                 };
      confidence: float64;
      lastAnalysis: text;
      name: text;
    }) query;
  suggestModification: (text, float64, float64, float64) ->
   (opt record {
          newStopLoss: float64;
          newTargetPrice: float64;
          reason: text;
        });
  voteOnStrategy: (text, variant {
                           Buy;
                           Hold;
                           Sell;
                         }, float64, float64) -> (bool, float64);
}
